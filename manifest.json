{
  "name": "SGP keyshort",
  "version": "1.0",
  "manifest_version": 2, 
  "description": "calculates a supergen password by pressing Ctrl + 1 or Ctrl + 2 to specify domain url",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
    },
  "content_scripts": [ {
    "js": [ "js/jquery.min.js", "js/sgp.hash.js", "js/sgp.core.js", "js/shortcut.js", "js/background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
    } ],
  "web_accessible_resources": ["js/jquery-1.10.2.min.map"],
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "file://*/*"
  ]
}